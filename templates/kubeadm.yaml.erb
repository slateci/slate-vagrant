apiVersion: kubeadm.k8s.io/v1alpha1
kind: MasterConfiguration
api:
  advertiseAddress: <%= api_advertise_ip %>
networking:
  podSubnet: <%= pod_cidr %>
  serviceSubnet: <%= service_cidr %>
apiServerCertSANs:
- 127.0.0.1
- <%= api_advertise_ip %>
apiServerExtraArgs:
  runtime-config: "api/all=true"
  endpoint-reconciler-type: lease
featureGates:
  CoreDNS: true
